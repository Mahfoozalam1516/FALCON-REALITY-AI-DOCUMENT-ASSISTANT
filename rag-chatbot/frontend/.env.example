# Frontend Environment Variables (place in frontend folder)
# Note: Vite environment variables must start with VITE_

# API Configuration
VITE_API_BASE_URL=http://localhost:8000

# App Configuration
VITE_APP_TITLE=AI Document Assistant
VITE_APP_SUBTITLE=FALCON REALITY ASSESSMENT

# Upload Configuration
VITE_MAX_FILE_SIZE_MB=10
VITE_MAX_FILES_PER_REQUEST=5
VITE_ALLOWED_FILE_TYPES=.pdf

# UI Configuration
VITE_TYPING_DELAY_MS=800
VITE_ANIMATION_DURATION_MS=600
VITE_ENABLE_ANIMATIONS=true

# Development Configuration
VITE_DEBUG=true
VITE_LOG_LEVEL=info